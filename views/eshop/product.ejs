        <td><%= row.kategori %></td>
        <td>
            <a href="product/update/<%= row.produktkod %>">Uppdatera</a>
            <span> | </span>
            <a href="product/delete/<%= row.produktkod %>">Ta bort</a>

        </td>
	</tr>
	<% }; %>
</table>

